{
  "name": "json-validator",
  "version": "0.1.0",
  "homepage": "https://cognitedata.github.io/cognite-remote-web-configurator/",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@cognite/gearbox": "^2.2.2",
    "@cognite/sdk": "^3.3.2",
    "ace-diff": "^3.0.3",
    "antd": "^3.14.1",
    "js-yaml": "^4.0.0",
    "jsoneditor": "^9.1.5",
    "lodash-es": "^4.17.15",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1",
    "swagger-parser": "^10.0.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "cross-env HTTPS=true SSL_CRT_FILE=.cert/cert.pem SSL_KEY_FILE=.cert/key.pem react-scripts start",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prod": "serve -s build --ssl-cert .cert/cert.pem --ssl-key .cert/key.pem",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/ace-diff": "^2.1.0",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^26.0.19",
    "@types/js-yaml": "^4.0.0",
    "@types/jsoneditor": "^8.6.0",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "node-sass": "^4.14.1",
    "react-scripts": "4.0.1",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.1.3"
  }
}
